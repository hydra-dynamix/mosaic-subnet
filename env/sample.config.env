# Mosaic Miner Configuration

# Network settings
COMX_USE_TESTNET=false

# Miner settings - Choose provider type
MOSAIC_MINER_PROVIDER=openai  # openai (for custom endpoints) or local (for local inference)

# For local provider (when MOSAIC_MINER_PROVIDER=local)
MOSAIC_MINER_MODEL=stabilityai/sdxl-turbo

# For custom endpoints (when MOSAIC_MINER_PROVIDER=openai)
MOSAIC_MINER_API_URL=https://api.example.com/v1
MOSAIC_MINER_API_KEY=your-key-here

# Network bindings
MOSAIC_MINER_HOST=0.0.0.0
MOSAIC_MINER_PORT=8080

# Validator settings - Choose model type
MOSAIC_VALIDATOR_MODEL=hps  # hps (local), clip (local), or api (remote)

# For CLIP model (when MOSAIC_VALIDATOR_MODEL=clip)
MOSAIC_VALIDATOR_CLIP_MODEL=laion/CLIP-ViT-L-14-laion2B-s32B-b82K

# For remote scoring (when MOSAIC_VALIDATOR_MODEL=api)
MOSAIC_VALIDATOR_API_URL=https://api.example.com/v1/score
MOSAIC_VALIDATOR_API_KEY=your-key-here

# Network bindings
MOSAIC_VALIDATOR_HOST=0.0.0.0
MOSAIC_VALIDATOR_PORT=8081

# Gateway settings
MOSAIC_GATEWAY_HOST=0.0.0.0
MOSAIC_GATEWAY_PORT=8082
